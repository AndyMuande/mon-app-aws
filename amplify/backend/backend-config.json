{
  "api": {
    "monapi": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "apimessages"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "function": {
    "apimessages": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ],
          "category": "storage",
          "resourceName": "messagesTable"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "hosting": {
    "amplifyhosting": {
      "providerPlugin": "awscloudformation",
      "service": "amplifyhosting",
      "type": "manual"
    }
  },
  "parameters": {
    "AMPLIFY_function_apimessages_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "apimessages"
        }
      ]
    },
    "AMPLIFY_function_apimessages_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "apimessages"
        }
      ]
    },
    "AMPLIFY_hosting_amplifyhosting_appId": {
      "usedBy": [
        {
          "category": "hosting",
          "resourceName": "amplifyhosting"
        }
      ]
    },
    "AMPLIFY_hosting_amplifyhosting_type": {
      "usedBy": [
        {
          "category": "hosting",
          "resourceName": "amplifyhosting"
        }
      ]
    }
  },
  "storage": {
    "messagesTable": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
